    "for file in Results:\n",
    "    \n",
    "    name = file\n",
    "    \n",
    "    print (name)\n",
    "\n",
    "    file = open(file, \"r\")\n",
    "    \n",
    "    for line in file:\n",
    "        entry_list =  line.strip().split()\n",
    "        entry_list.append(name)\n",
    "        \n",
    "        print (entry_list)\n",
    "        \n",
    "        master_list.append(entry_list)\n",
    "        \n",
    "        \n",
   "    file.close()"
